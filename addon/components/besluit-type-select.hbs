<PowerSelect
  @disabled={{@disabled}}
  @placeholder="Jaarrekening, ..."
  @search={{this.search}}
  @searchPlaceholder="Typ om te zoeken..."
  @searchEnabled={{true}}
  @options={{@besluitTypes}}
  @selected={{@selected}}
  @onChange={{@onchange}} as |besluitType|>
  {{besluitType.label}}
</PowerSelect>
